<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>j2ee2</display-name>
  <servlet>
    <servlet-name>s1</servlet-name>
    <servlet-class>mypack.Hello</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>s1</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>s2</servlet-name>
    <servlet-class>mypack.Hello2</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>s2</servlet-name>
    <url-pattern>/hello/tt</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>mypack.Register</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>processForm</servlet-name>
    <servlet-class>mypack.ProcessForm</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>processForm</servlet-name>
    <url-pattern>/processForm1</url-pattern>
  </servlet-mapping>
  <filter>
  <filter-name>f1</filter-name>
  <filter-class>mypack.MyFilter</filter-class>
  </filter>
  <filter-mapping>
  <filter-name>f1</filter-name>
	<!--   <servlet-name>s1</servlet-name> -->
	<url-pattern>/*</url-pattern>
  </filter-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>